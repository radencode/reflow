{
	"name": "reflow",
	"version": "2.0.0",
	"description": "reflow file management sofware",
	"main": "./app/main.js",
	"scripts": {
		"start": "electron ./app/main.js",
		"bundle": "webpack --mode=production --config ./webpack/webpack.production.babel.js",
		"electron": "electron ./server/main.js",
		"api-server": "nodemon ./server/server.js",
		"clint-server": "webpack-dev-server --mode=development --config ./webpack/webpack.development.babel.js",
		"dev": "concurrently \"npm run clint-server\" \" npm run api-server\" \" npm run electron\"",
		"build": "npm run bundle && electron-packager ./app Reflow --electron-version 1.8.2 --platform win32 --overwrite --asar true --icon ./src/icons/electron/icon.ico --out ./release"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/radencode/reflow.git"
	},
	"author": "Teddy Radenkov <teddy.pdx@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/radencode/reflow/issues"
	},
	"homepage": "https://github.com/radencode/reflow#readme",
	"dependencies": {
		"electron": "^1.8.2",
		"electron-builder": "^20.5.1",
		"material-design-icons": "^3.0.1",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-redux": "^5.0.7",
		"react-router-dom": "^4.2.2",
		"react-transition-group": "^2.3.1",
		"react-virtualized": "^9.18.5",
		"redux": "^3.7.2",
		"redux-promise-middleware": "^5.1.1"
	},
	"devDependencies": {
		"axios": "^0.18.0",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.2",
		"babel-loader": "^7.1.2",
		"babel-plugin-react-html-attrs": "^2.1.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"body-parser": "^1.18.2",
		"clean-webpack-plugin": "^0.1.18",
		"concurrently": "^3.5.1",
		"copy-webpack-plugin": "^4.4.1",
		"cors": "^2.8.4",
		"css-loader": "^0.28.9",
		"edge-js": "^9.3.4",
		"electron-packager": "^11.0.1",
		"eslint": "^4.18.2",
