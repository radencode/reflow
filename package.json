		"file-loader": "^1.1.6",
		"html-webpack-plugin": "^3.2.0",
		"node-sass": "^4.7.2",
		"nodemon": "^1.15.1",
		"progress-bar-webpack-plugin": "^1.10.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.2.0",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.20.2",
		"uglifyjs-webpack-plugin": "^1.2.0",
		"url-loader": "^0.6.2",
		"webpack": "^4.8.3",
		"webpack-cli": "^2.1.3",
